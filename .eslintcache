 (event) => {\n    this.setState({\n      [event.target.name]: event.target.value,\n    });\n  };\n\n  toggleSignup = () => {\n    this.setState((prevState) => {\n      return {\n        showSignup: !prevState.showSignup,\n      };\n    });\n  };\n\n  handleSubmitClick = (event) => {\n    event.preventDefault();\n    const { email, password } = this.state;\n    if (password === \"\" || email === \"\") {\n      NotificationManager.error(\"\", \"Please enter a valid email/password\");\n      return;\n    }\n    if (password.length <= 7) {\n      NotificationManager.error(\n        \"\",\n        \"Please enter a password of atleast 8 characters\"\n      );\n      return;\n    }\n    const re = /^(([^<>()[\\]\\\\.,;:\\s@\\\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\\\"]+)*)|(\\\".+\\\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\n    const isEmailValid = re.test(email);\n\n    if (!isEmailValid) {\n      NotificationManager.error(\"\", \"Please enter a valid email\");\n      return;\n    }\n    this.setState({\n      loading: true\n    });\n    this.handleLogin();\n  };\n\n  handleLogin = () => {\n    const {\n      email, password\n    } = this.state;\n    fire\n      .auth()\n      .signInWithEmailAndPassword(email, password)\n      .then(userRecord => {\n        console.log(userRecord);\n        this.setState({\n          loading: false,\n          email: '',\n          password: ''\n        });\n        this.props.history.push('/chatlist');\n      })\n      .catch(error => {\n        NotificationManager.error('', error.message);\n      });\n  } \n\n  authListner = () => {\n    fire.auth().onAuthStateChanged(user => {\n      if(user){\n        this.props.setUser(user);\n      }else{\n        this.props.setUser('');\n      }\n    })\n  }\n\n  componentDidMount() {\n    this.authListner();\n  }\n\n  render() {\n    const { email, password, showSignup, loading } = this.state;\n    let disableSubmit = loading ? 'disabled' : '';\n\n    if(this.props.user !== '') {\n      return <Redirect to='/chatlist' />\n    }\n\n    return (\n      <div>\n        <div className=\"shadow title-container come-down-bounce bg-primary pt-5 pb-5 p-0 m-0 col-12\">\n          <p className=\"text-light m-0 p-0 letter-spacing-1 font-weight-bold font-xlarge text-align-center\">\n            Chatting Box\n          </p>\n        </div>\n        <div className=\"col-11 mx-auto login\">\n          <div className=\"mt-5\">\n            <p className=\"text-align-center font-large font-weight-bold\">\n              Login\n            </p>\n          </div>\n          <form action=\"\">\n            <div className=\"input-containers mt-4 bg-very-light-gray p-3 rounded-12\">\n              <label\n                htmlFor=\"email-login\"\n                className=\"font-small text-secondary\"\n              >\n                Your email\n              </label>\n              <input\n                onChange={this.handleInputChange}\n                value={email}\n                id=\"email-login\"\n                maxLength={40}\n                className=\"font-weight-bold p-0 form-control border-0 bg-transparent\"\n                type=\"text\"\n                name=\"email\"\n                placeholder=\"Enter email id\"\n              />\n            </div>\n            <div className=\"input-containers mt-4 bg-very-light-gray p-3 rounded-12\">\n              <label\n                htmlFor=\"password-login\"\n                className=\"font-small text-secondary\"\n              >\n                Your password\n              </label>\n              <input\n                onChange={this.handleInputChange}\n                value={password}\n                maxLength={30}\n                className=\"form-control border-0 bg-transparent p-0\"\n                type=\"password\"\n                name=\"password\"\n                id=\"password-login\"\n                placeholder=\"password\"\n              />\n            </div>\n            <div className=\"mt-4 mx-auto col-10\">\n              <button\n                onClick={this.handleSubmitClick}\n                type=\"submit\"\n                className=\"col-12 btn btn-primary form-control\"\n                disabled={disableSubmit}\n              >\n                Login\n                {loading && <i class=\"ml-2 fas fa-circle-notch fa-spin\"></i>}\n              </button>\n            </div>\n          </form>\n          <div className=\"mt-3 d-flex justify-content-end\">\n            <p\n              onClick={this.toggleSignup}\n              className=\"font-small text-primary pointer\"\n            >\n              Don't have an account? Sign up\n            </p>\n          </div>\n        </div>\n        {showSignup && <SignupContainer toggleSignup={this.toggleSignup} />}\n      </div>\n    );\n  }\n}\n\nexport default withRouter(LoginContainer);\n","/home/shalini/Documents/react-dev/chatting-clone/src/components/SignupContainer.jsx",["57","58","59","60"],"/home/shalini/Documents/react-dev/chatting-clone/src/components/ChatsContainer.jsx",[],"/home/shalini/Documents/react-dev/chatting-clone/src/components/ChatSent.jsx",[],{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","replacedBy":"64"},{"ruleId":"61","replacedBy":"65"},{"ruleId":"63","replacedBy":"66"},{"ruleId":"61","replacedBy":"67"},{"ruleId":"63","replacedBy":"68"},{"ruleId":"69","severity":1,"message":"70","line":1,"column":28,"nodeType":"71","messageId":"72","endLine":1,"endColumn":37},{"ruleId":"73","severity":1,"message":"74","line":48,"column":38,"nodeType":"75","messageId":"76","endLine":48,"endColumn":39,"suggestions":"77"},{"ruleId":"73","severity":1,"message":"74","line":48,"column":63,"nodeType":"75","messageId":"76","endLine":48,"endColumn":64,"suggestions":"78"},{"ruleId":"73","severity":1,"message":"74","line":48,"column":72,"nodeType":"75","messageId":"76","endLine":48,"endColumn":73,"suggestions":"79"},{"ruleId":"73","severity":1,"message